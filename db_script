delete from user_sessions where user_id in (select user_id from users where email = 'abc@gmail.com');
delete from user_activities where user_id in (select user_id from users where email = 'abc@gmail.com');
delete from users where email = 'abc@gmail.com';
delete from organizations where name = 'ABC Corp';
delete from tenants where name = 'ABC Corp';

